{
  "name": "example",
  "version": "1.0.0",
  "description": "Example app for react-ssr-starter",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=development babel-node src",
    "debug": "NODE_ENV=development babel-node --inspect=9229 src",
    "build": "rm -rf build && npm run build:server && npm run build:assets",
    "build:server": "NODE_ENV=production babel src --out-dir build --copy-files",
    "build:assets": "NODE_ENV=production webpack --progress --color -p --config webpack.config.js",
    "start": "NODE_ENV=production node build",
    "clean": "rm -rf build && rm -rf node_modules"
  },
  "author": "Emanuele Longo",
  "license": "ISC",
  "dependencies": {
    "react": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-ssr-starter": "1.0.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.21.0"
  }
}
